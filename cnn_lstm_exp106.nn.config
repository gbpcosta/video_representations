[MODEL]
model_id = 106
model_name = cnn_lstm_clf_small
model_type = cnn_lstm_clf_small
ex_mode = eval_all

[DATASET]
dataset_name = bouncingMNIST
class_velocity = True
hide_digits = True
tr_size = 8000
val_size = 4000

[NETWORK ARCHITECTURE]
use_batch_norm = True
use_layer_norm = False
use_l2_reg = False
pretrained_cnn = mobilenet

[TRAINING]
epoch = 50
batch_size = 32
learning_rate = 1e-5

[VISUALIZATION]
svm_analysis = True
vis_epoch = 5
plot_individually = True
verbosity = 1

[CHECKPOINT]
checkpoint_epoch = 5
keep_checkpoint_max = 2
redo = False

[INTEGRATION]
slack_bot = True

[DIRECTORIES]
plt_dir = _outputs/{}/plots
model_dir = _outputs/{}/models
config_dir = _outputs/{}/configs

[HARDWARE]
gpu_id = 0
